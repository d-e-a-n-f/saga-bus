{
  "name": "@saga-bus/transport-sqs",
  "version": "0.0.1",
  "description": "AWS SQS transport for saga-bus",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist", "README.md"],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deanforan/saga-bus.git",
    "directory": "packages/transport-sqs"
  },
  "keywords": ["saga", "message-bus", "transport", "sqs", "aws", "fifo"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@saga-bus/core": "workspace:*",
    "@aws-sdk/client-sqs": "^3.600.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@testcontainers/localstack": "^10.0.0",
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.2",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "@aws-sdk/client-sqs": ">=3.0.0"
  }
}
