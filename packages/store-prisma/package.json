{
  "name": "@saga-bus/store-prisma",
  "version": "0.1.5",
  "description": "Prisma ORM adapter for saga-bus state storage",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "prisma",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/d-e-a-n-f/saga-bus.git",
    "directory": "packages/store-prisma"
  },
  "bugs": {
    "url": "https://github.com/d-e-a-n-f/saga-bus/issues"
  },
  "homepage": "https://github.com/d-e-a-n-f/saga-bus#readme",
  "keywords": [
    "saga",
    "message-bus",
    "store",
    "prisma",
    "orm"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@saga-bus/core": "workspace:*"
  },
  "devDependencies": {
    "@prisma/client": "^6.8.0",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "prisma": "^6.8.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@prisma/client": ">=5.0.0"
  }
}
