{
  "name": "@saga-bus/transport-gcp-pubsub",
  "version": "0.2.2",
  "description": "Google Cloud Pub/Sub transport for saga-bus",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@saga-bus/core": "workspace:*"
  },
  "peerDependencies": {
    "@google-cloud/pubsub": ">=4.0.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^22.10.1",
    "eslint": "^9.16.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/d-e-a-n-f/saga-bus.git",
    "directory": "packages/transport-gcp-pubsub"
  },
  "bugs": {
    "url": "https://github.com/d-e-a-n-f/saga-bus/issues"
  },
  "homepage": "https://github.com/d-e-a-n-f/saga-bus#readme",
  "keywords": [
    "saga-bus",
    "saga",
    "gcp",
    "pubsub",
    "google-cloud",
    "transport",
    "message-bus"
  ]
}
