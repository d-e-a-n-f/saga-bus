{
  "name": "saga-bus",
  "description": "A MassTransit-style saga orchestration library for TypeScript/Node.js",
  "private": true,
  "license": "MIT",
  "author": "Dean Foran",
  "repository": {
    "type": "git",
    "url": "https://github.com/d-e-a-n-f/saga-bus.git"
  },
  "homepage": "https://github.com/d-e-a-n-f/saga-bus#readme",
  "bugs": {
    "url": "https://github.com/d-e-a-n-f/saga-bus/issues"
  },
  "keywords": [
    "saga",
    "orchestration",
    "state-machine",
    "workflow",
    "event-driven",
    "microservices",
    "message-bus",
    "typescript",
    "rabbitmq",
    "kafka",
    "sqs",
    "redis",
    "nats"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "typecheck": "turbo run check-types",
    "clean": "turbo run clean && rm -rf node_modules",
    "release": "node scripts/release.mjs"
  },
  "devDependencies": {
    "prettier": "^3.7.4",
    "turbo": "^2.6.3",
    "typescript": "5.9.2",
    "vitest": "^3.0.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=20"
  }
}
