{
  "name": "@saga-bus/example-stores",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "description": "Example demonstrating different saga stores: PostgreSQL, MySQL, SQL Server, MongoDB, Redis",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "tsx src/index.ts",
    "demo:postgres": "tsx src/postgres-demo.ts",
    "demo:mysql": "tsx src/mysql-demo.ts",
    "demo:sqlserver": "tsx src/sqlserver-demo.ts",
    "demo:mongo": "tsx src/mongo-demo.ts",
